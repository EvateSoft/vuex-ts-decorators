{
  "name": "vuex-ts-decorators",
  "version": "0.0.6",
  "description": "TypeScript decorators for working with Vuex",
  "main": "src/index.ts",
  "scripts": {
    "preversion": "npm test",
    "postversion": "git push --tags && npm publish",
    "test": "mocha -r source-map-support/register -r ts-node/register -r es6-promise/auto test/**/*.ts",
    "start": "npm run test -- -w --watch-extensions ts"
  },
  "repository": "snaptopixel/vuex-ts-decorators",
  "author": "Casey Corcoran <github@snaptopixel.com> (http://snaptopixel.com)",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/mocha": "2.2.39",
    "@types/sinon": "1.16.35",
    "chai": "3.5.0",
    "es6-promise": "4.0.5",
    "mocha": "3.2.0",
    "sinon": "1.17.7",
    "source-map-support": "0.4.11",
    "ts-node": "2.1.0",
    "typescript": "2.2.1",
    "vue": "2.2.1",
    "vuex": "2.2.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
